<Window x:Name="RaBoTa_ru" x:Class="mebel_centr.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:mebel_centr"
        mc:Ignorable="d"
        Title="mebel" MinHeight="450" MinWidth="800" Icon="/Resources/mebelqwe.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="60"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="#E8ADAA">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="115*"/>
                <ColumnDefinition Width="27*"/>
                <ColumnDefinition Width="658*"/>
            </Grid.ColumnDefinitions>
            <Image Width="45" Height="45" HorizontalAlignment="Left" Margin="5,8,0,7" Source="/Resources/mebel.png"/>
            <Button x:Name="btnBack" HorizontalAlignment="Right" Content="Назад" Click="btnBack_Click" Grid.Column="2" Margin="538,10,0,10"/>
        </Grid>
        <Frame x:Name="FrmMain" Grid.Row="1" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"/>
    </Grid>
</Window>


